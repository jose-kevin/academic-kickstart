.photography-html{scroll-snap-type:y mandatory;scroll-padding-top:calc(70px + 10vh)}.photography-container{overflow-y:scroll;height:100vh}.photography-section{align-items:flex-start;display:flex;flex-direction:row;flex-wrap:nowrap;justify-content:center;height:calc(100vh - 70px);padding-left:100px;padding-right:100px;scroll-snap-align:start}.photography-image{max-height:calc(80vh - 70px)}.photography-text{flex-shrink:1;text-align:right;min-width:300px;margin-right:40px;max-width:500px}.photography-section:first-of-type{margin-top:calc(70px + 10vh)}